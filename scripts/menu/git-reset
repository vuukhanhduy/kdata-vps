#!/bin/sh

echo "-------------------------------------------------------------------------"
echo "Ok ! please wait check reset code from git..."

cd ~
if [ -d /opt/echbay_kdata ]; then
rm -rf /opt/echbay_kdata/*
rm -rf /opt/echbay_kdata
sleep 5
fi

/etc/kdata/menu/git-clone
sleep 1

clear
echo "========================================================================="
echo "Done! Reset code from git..."
/etc/kdata/menu/kdata-update-upgrade-service-menu
